{
  "name": "@cmfcmf/docusaurus-search-local",
  "version": "0.1.1",
  "description": "Local / Offline Search for Docusaurus",
  "keywords": [
    "docusaurus",
    "search",
    "plugin",
    "offline",
    "local"
  ],
  "license": "MIT",
  "repository": "cmfcmf/docusaurus-search-local",
  "main": "src/index.js",
  "publishConfig": {
    "access": "public"
  },
  "author": "Christian Flach",
  "dependencies": {
    "autocomplete.js": "^0.37.0",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "lunr": "^2.3.8",
    "lunr-languages": "^1.4.0"
  },
  "peerDependencies": {
    "@docusaurus/core": "^2.0.0"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "lint": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=10.9.0"
  }
}
